   "source": [
    "import scipy.stats as stats\n",
    "mu = 100  \n",
    "sigma = 15 \n",
    "prob = stats.norm.cdf(115, mu, sigma) - stats.norm.cdf(85, mu, sigma)\n",
    "percentage = prob * 100\n",
    "print(f\"Approximately {percentage:.2f}% of the data lies between 85 and 115.\")"
   ]
  }
